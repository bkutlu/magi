block belowTheFold
	script(src='components/jquery/dist/jquery.min.js')
	script.
		var imgs = [ "brown", "nsf", "tcga", "hint", "hprd", "irefindex", "pfam", "smart", "cd"];
		imgs.forEach(function(name){
			$("img#" + name + "-gray").mouseover(function(){
				$(this).hide();
				$("img#" + name + "-color").show();
			});
			$("img#" + name + "-color").mouseout(function(){
				$(this).hide();
				$("img#" + name + "-gray").show();
			});
		});
